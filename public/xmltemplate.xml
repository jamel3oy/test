<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
    <w:body>
        {{#categories}}
        <w:p>
            <w:r>
                <w:t>{{category_name}}</w:t>
            </w:r>
        </w:p>
        {{#items}}
        <w:p>
            <w:r>
                <w:t>{{item_name}}</w:t>
            </w:r>
        </w:p>
        {{/items}}
        {{/categories}}
    </w:body>
</w:document>
